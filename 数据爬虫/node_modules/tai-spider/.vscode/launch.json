{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "request": "launch",
            "type": "pwa-node",
            "program": "${file}"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Spider",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/taispider.js",
            "args": [
                "run",
                "quotes"
            ],
        }
    ]
}